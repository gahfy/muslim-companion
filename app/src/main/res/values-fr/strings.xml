    <!-- Format de date grégorienne -->
    <!-- %2$d est pour le numéro du jour -->
    <!-- %1$s est pour le nom du mois -->
    <!-- %3$d est pour le numéro de l'année -->
    <!-- %4$s est pour le suffixe du jour -->
    <!-- %2$d%4$s %1$s %3$d => 1er Mai 2015 -->
    <!-- %1$s %2$d%4$s %3$d => Mai 1er 2015 -->
    <!-- %3$d %1$s %2$d%4$s => 2015 Mai 1er -->
    <string name="gregorian_date_format" formatted="false">%2$d%4$s %1$s %3$d</string>

    <!-- Format de date islamique -->
    <!-- %1$d est pour le numéro du jour -->
    <!-- %2$s est pour le nom du mois -->
    <!-- %3$d est pour le numéro de l'année -->
    <!-- %1$d %2$s %3$d => 2 Chaabane 1436 -->
    <!-- %2$s %1$d %3$d => Chaabane 2 2015 -->
    <!-- %3$d %2$s %1$d => 2015 Mai 2 -->
    <string name="islamic_date_format" formatted="false">%1$d %2$s %3$d</string>

